<?xml version="1.0" encoding="UTF-8"?>
<Protocol name="BasicTest" id="100" proxy="GameServer" comment="基本测试">
	<TypeDef>
	</TypeDef>
	<FuncC2S client="ActionScript">
		<Func name="ReqEnterGame" ret="true" comment="请求进入游戏">
			<RetParam type="UINT64" name="qwUsrID" comment="UserID返回"/>
		</Func>
		<Func name="ReqTestResult" ret="true" comment="请求测试">
			<Param type="INT32" name="dwRand" keep="true" comment="随机数"/>
