			<RetParam type="UINT64" name="qwUsrID" comment="UserID"/>
			<RetParam type="string" name="strRet" comment="随机数文本返回"/>
		</Func>
	</FuncC2S>
	<FuncS2C>
		<Func name="TimerNotify" comment="时钟触发通知">
			<Param type="UINT64" name="qwUsrID" comment="UserID"/>
			<Param type="UINT32" name="dwTime" comment="当前时间"/>
		</Func>
	</FuncS2C>
</Protocol>
