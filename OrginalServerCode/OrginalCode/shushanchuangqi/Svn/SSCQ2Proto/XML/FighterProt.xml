    <FuncC2S client="ActionScript,PHP">
        <Func name="FighterSkillEquipTo" ret="true" comment="散仙装备主动技能">
            <Param type="UINT64" name="qwInstID" comment="散仙唯一ID"/>
            <Param type="UINT32" name="dwSkillID" comment="技能ID"/>
            <Param type="INT32" name="iSlot" comment="技能槽 -1表示卸下技能"/>
            <RetParam type="ESkillResult" name="eResult" comment="结果"/>
            <RetParam type="TVecFightSkillInfo" name="vActiveSkill" comment="装备的主动技能"/>
        </Func>
        <Func name="FighterSkillLevelUp" ret="true" comment="散仙技能升级">
            <Param type="UINT64" name="qwInstID" comment="散仙唯一ID"/>
            <Param type="UINT32" name="dwSkillID" comment="技能ID"/>
            <RetParam type="ESkillResult" name="eResult" comment="结果"/>
        </Func>
        <Func name="FighterPassiveSkillLevelUp" ret="true" comment="散仙被动技能升级">
            <Param type="UINT64" name="qwInstID" comment="散仙唯一ID"/>
            <Param type="UINT32" name="dwSkillID" comment="技能ID"/>
            <RetParam type="ESkillResult" name="eResult" comment="结果"/>
        </Func>
        <Func name="GetTavernInfo" ret="true" comment="获取客栈信息" >
            <RetParam type="UINT32"     name="dwDrawCount" comment="免费求签次数" />
            <RetParam type="TVecTavernInfo" name="vecTavernInfo" comment="具体客栈信息" />
        </Func>
        <Func name="Draw" ret="true" comment="求签" >
            <Param      type="EDrawType"                name="eType"        comment="求签类型" />
            <RetParam   type="EDrawResult"              name="eResult"      comment="返回结果" />
            <RetParam   type="ItemProt::TVecItemCount"  name="vecItemGet"   comment="求签获得物品" />
        </Func>
        <Func name="FighterRecruit" ret="true" comment="招募">
            <Param type="UINT16" name="wFighterID" comment="将ID"/>
            <RetParam type="ERecruitResult" name="eResult" comment="结果"/>
        </Func>
        <Func name="CuiTiUpgrade" ret="true" comment="淬体升级">
            <Param type="UINT64" name="qwInstID" comment="将唯一ID"/>
            <Param type="ECuiTiType" name="eCuiTiType" comment="淬体类型"/>
            <Param type="UINT8" name="byTimes" comment="次数"/>
            <RetParam type="ECuiTiResult" name="eResult" comment="结果"/>
            <RetParam type="UINT8" name="byLevel" comment="当前淬体等级"/>
        </Func>
        <Func name="CuiTiAllUpgrade" ret="true" comment="散仙淬体全体(六种)升级一次">
            <Param type="UINT64" name="qwInstID" comment="将唯一ID"/>
            <RetParam type="ECuiTiResult" name="eResult" comment="结果"/>
            <RetParam type="TVecUINT8" name="vecLevel" comment="当前全体淬体等级(按炼皮/通筋/强肌/锻骨/活络/洗髓顺序)"/>
        </Func>
        <Func name="FighterShift" ret="true" comment="转移">
            <Param type="UINT64" name="qwInstIDSrc" comment="将唯一ID"/>
            <Param type="UINT64" name="qwInstIDDst" comment="将唯一ID"/>
            <RetParam type="EShiftResult" name="eResult" comment="结果"/>
        </Func>
        <Func name="FighterSyncLevel" ret="true" comment="同步等级">
            <Param type="UINT64" name="qwInstID" comment="将唯一ID"/>
            <Param type="UINT8" name="byGoalLvl" comment="同步的目标等级"/>
            <RetParam type="ESyncLevelResult" name="eResult" comment="结果"/>
        </Func>
        <Func name="UpgradeDujie" ret="true" comment="渡劫升级">
            <Param type="UINT64" name="qwInstID" comment="将唯一ID"/>
            <Param type="UINT8"  name="byOneKey" comment="是否一键(是>0 否=0)"/>
            <RetParam type="EDujieResult" name="eResult" comment="结果"/>
            <RetParam type="UINT64" name="qwInstID" comment="将唯一ID"/>
        </Func>
        <Func name="ReqFighterXinFaInfo" ret="true" comment="请求散仙心法">
            <Param type="UINT64" name="qwInstID" comment="散仙唯一ID"/>
            <RetParam type="TVecXinFaInfo" name="vecFighterXinFa" comment="散仙已经学会的心法信息"/>
        </Func>
        <Func name="FighterActiveXinFa" ret="true" comment="散仙学习心法">
            <Param type="UINT64" name="qwInstID" comment="散仙唯一ID"/>
            <Param type="UINT32" name="dwXinFaID" comment="心法ID"/>
            <RetParam type="EXinFaResult" name="eResult" comment="结果"/>
        </Func>
        <Func name="FighterXinFaLevelUp" ret="true" comment="散仙心法升级">
