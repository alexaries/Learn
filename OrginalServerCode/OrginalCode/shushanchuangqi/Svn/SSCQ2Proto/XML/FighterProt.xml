        <Func name="FighterXinFaDisperse" ret="true" comment="散仙心法散功">
            <Param type="UINT64" name="qwInstID" comment="散仙唯一ID"/>
            <Param type="UINT32" name="dwXinFaID" comment="心法ID"/>
            <RetParam type="EXinFaResult" name="eResult" comment="结果"/>
        </Func>
        <Func name="GetAllFighterCombination" ret="true" comment="获取玩家的所有散仙组合">
            <RetParam type="TVecFighterCombination" name="vecCombinationInfo" comment="玩家的散仙组合信息"/>
        </Func>
        <Func name="ActiveCombination" ret="true" comment="激活散仙组合">
            <Param type="UINT8" name="byGroupID" comment="散仙组合ID"/>
