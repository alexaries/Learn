            <Member type="UINT8"             name="bystatus"     comment="状态" />
            <Member type="string"            name="strHelpName"      comment="帮助玩家的名字" />
            <Member type="UINT8"             name="byHelpColor"      comment="帮助者名字颜色" /> 
        </Struct>
        <Struct name="SGuildExchangedItem" vec="TVecGuildExchangedItem" comment="帮派兑换物品">
