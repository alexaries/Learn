    <FuncC2S client="ActionScript,PHP">
        <Func name="GetStore" ret="true" comment="请求包裹信息(整个包裹)">
            <Param type="EStoreType" name="eStoreType" keep="true" comment="存储类型"/>
            <RetParam type="SStoreStream" name="stStoreStream" comment="背包信息"/>
            <RetParam type="UINT32" name="dwNextUnlockTime" comment="下一个解锁背包空间的时间 0-表示可解锁 0xFFFFFFFF-表示没有可解锁的了 其他为秒"/>
        </Func>
        <Func name="UseItem" ret="true" comment="请求使用包裹中的道具">
            <Param type="EStoreType" name="eStoreType" keep="true" comment="存储类型"/>
            <Param type="UINT64" name="qwRoleID" keep="true" comment="针对的角色ID"/>
            <Param type="UINT64" name="qwMercenID" keep="true" comment="针对的佣兵ID"/>
            <Param type="UINT64" name="qwInstID" keep="true" comment="优先使用的物品实例ID"/>
            <Param type="UINT16" name="wStack" keep="true" comment="使用个数(只针对堆叠道具)"/>
            <!--LocParam type="UINT16" name="wItemID" keep="true" comment="道具模板ID"/-->
            <RetParam type="EItemResult" name="eResult" comment="使用道具结果"/>
        </Func>
        <Func name="ExpandStore" ret="true" comment="扩充包裹或仓库">
            <Param type="EStoreType" name="eStoreType" comment="存储类型"/>
            <Param type="UINT8" name="byCount" comment="扩充个数"/>
            <Param type="ProtoCommon::ECoinType" name="eCoinType" comment="货币类型"/>
            <RetParam type="EItemResult" name="eResult" comment="扩充结果"/>
        </Func>
        <Func name="RelocateStoreItem" ret="true" comment="包裹或仓库中道具换位置">
            <Param type="EStoreType" name="eStoreType" keep="true" comment="存储类型"/>
            <Param type="UINT64" name="qwInstID" keep="true" comment="道具的实例ID"/>
            <Param type="INT16" name="wNewIndex" comment="道具在包裹中新的位置0开始"/>
            <RetParam type="EItemResult" name="eResult" comment="换位置的结果"/>
        </Func>
        <Func name="TidyupStore" ret="true" comment="整理包裹或仓库">
            <Param type="EStoreType" name="eStoreType" keep="true" comment="存储类型"/>
            <RetParam type="EItemResult" name="eResult" comment="整理的结果"/>
        </Func>
        <Func name="SplitStackItem" ret="true" comment="拆分包裹或仓库中的堆叠类道具">
            <Param type="EStoreType" name="eStoreType" keep="true" comment="存储类型"/>
            <Param type="UINT64" name="qwInstID" keep="true" comment="道具的实例ID"/>
            <Param type="UINT16" name="wStack" comment="拆分出来的堆叠数量"/>
            <Param type="INT16" name="wIndex" comment="拆分出来的堆叠道具的存储位置(-1表示选第一个空位)"/>
            <RetParam type="EItemResult" name="eResult" comment="拆分的结果"/>
        </Func>
        <Func name="MergeStackItem" ret="true" comment="合并包裹或仓库中的堆叠类道具">
            <Param type="EStoreType" name="eStoreType" keep="true" comment="存储类型"/>
            <Param type="UINT64" name="qwFromInstID" keep="true" comment="源道具的实例ID"/>
            <Param type="UINT64" name="qwToInstID" keep="true" comment="目的道具的实例ID"/>
            <RetParam type="EItemResult" name="eResult" comment="合并的结果"/>
        </Func>
        <Func name="DestroyStoreItem" ret="true" comment="销毁包裹或仓库中的道具">
            <Param type="EStoreType" name="eStoreType" keep="true" comment="存储类型"/>
            <Param type="UINT64" name="qwInstID" keep="true" comment="道具的实例ID"/>
            <RetParam type="EItemResult" name="eResult" comment="销毁的结果"/>
        </Func>
        <Func name="TransferItem" ret="true" comment="在包裹和仓库之间移动道具">
            <Param type="EStoreType" name="eFromStoreType" comment="源存储类型"/>
            <Param type="EStoreType" name="eToStoreType" comment="目的存储类型"/>
            <Param type="UINT64" name="qwInstID" comment="道具的实例ID"/>
            <Param type="INT16" name="wIndex" comment="道具在目的存储中的位置(-1表示选第一个空位)"/>
            <RetParam type="EItemResult" name="eResult" comment="移动的结果"/>
